{
  "root": true,
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-use-before-define": 0,
    "global-require": 0,
    "no-unused-vars": 1,
    "no-shadow": 1,
    "camelcase": 1,
    "no-empty": 1,
    "prefer-const": 1,
    "no-plusplus": 0,
    "no-continue": 0,
    "prettier/prettier": 1,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "@typescript-eslint/no-unused-vars": 1,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0
  }
}
